{
  "name": "@cortexide/api",
  "version": "0.1.0",
  "description": "Cortexide REST API server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:migrate": "bash scripts/migrate.sh",
    "db:seed": "bash scripts/seed.sh"
  },
  "dependencies": {
    "@cortexide/core": "workspace:*",
    "fastify": "^4.25.1",
    "@fastify/cors": "^8.5.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "pino": "^8.16.2",
    "pino-pretty": "^10.3.0"
  },
  "devDependencies": {
    "@types/pg": "^8.10.9",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}
